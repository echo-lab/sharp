{
  "atom-workspace atom-text-editor:not([mini])[data-grammar~='tidalcycles']": {
    "ctrl-alt-x": "sharp:toggle",
    "ctrl-enter": "sharp:enter",
    "cmd-enter" : "sharp:enter",
    "cmd-alt-shift-enter" : "sharp:enterAll",
    "ctrl-alt-shift-enter" : "sharp:enterAll",
    "ctrl-alt-u": "sharp:unique",
    "ctrl-alt-cmd-s": "sharp:dumpHistory"
  }
}
